@model bookingadmintest.Models.Workout

@{
    ViewData["Title"] = "Details";
}

<h1 class="mt-5 h2 text-center">Detaljer</h1>

<h3 class="h4 text-center">Träningspass</h3>
<div class="card p-3 mb-3 mt-5" style="max-width: 800px;">
    <h4 class="text-center">@Html.DisplayFor(model => model.WorkoutName)</h4>
    <ul class="list-unstyled mx-5">
        <li>
            @Html.DisplayNameFor(model => model.WorkoutDate): @Html.DisplayFor(model => model.WorkoutDate)
        </li>
        <li>

            @Html.DisplayNameFor(model => model.Instructor): @Html.DisplayFor(model => model.Instructor)
        </li>
        <li>
            @Html.DisplayNameFor(model => model.Duration): @Html.DisplayFor(model => model.Duration) min
        </li>
        <li>
            @Html.DisplayNameFor(model => model.Cost): @Html.DisplayFor(model => model.Cost) :-
        </li>
        <li>
            @Html.DisplayNameFor(model => model.Quantity): @Html.DisplayFor(model => model.Quantity) st
        </li>
        <li>
            @Html.DisplayNameFor(model => model.BookedQuantity): @Html.DisplayFor(model => model.BookedQuantity) st
        </li>
    </ul>
    <div class="text-center">
        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id">Ändra</a> |
        <a class="btn btn-secondary" asp-action="Index">Tillbaka</a>
    </div>
</div>

